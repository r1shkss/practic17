using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace viper
{
    public class Order
    {
        public int ID { get; set; }
        public decimal Total { get; set; }
    }
}
internal class Program
{
    static void Main(string[] args)
    {
        var oreder1 = new Order { ID = 1001, Total = 1500.50m };
        var oreder2 = oreder1;
        oreder2.Total = 2000.00m;
        Console.WriteLine(oreder1.Total);
    }
}
